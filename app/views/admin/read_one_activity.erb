<h2><%=@activity.event_name%></h2>

      
<table border="1">
  <thead>
    <tr>
      <th>Registered Students</th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td>
        <%if @results == []%>
          No Students Registered Yet 
        <%else @results.each do |r| %>
                <%= r.access_student.first_name %> 
                <%= r.access_student.last_name %>
                <br>
                <%end%>
        <%end%>
      </td>
    </tr>
  </tbody>
</table>